\subsection[Nester]{Nester der Vespa velutina}

\begin{frame}{Nester der Vespa velutina}
\frametitle{Nester der Vespa velutina} 
\framesubtitle{Die verschiedenen Nester der Hornisse}

\begin{columns}[t]
    \column{0.45\textwidth}
    \begin{enumerate}
        \item \textbf{Embryonal-Nester}: Die Jungkönigin zieht die ersten Arbeiterinnen auf
        \item \textbf{Primär-Nest}: Das Volk wird weiter aufgebaut...
        \item \textbf{Sekundärnest}: ... und erreicht dann seine volle Größe
    \end{enumerate}	
    \column{0.5\textwidth}
    \begin{overlayarea}{\textwidth}{0.45\textheight}
        \only<1->{\includegraphics[width=0.45\textwidth]{figures/Altmeyer1.jpg}}
        \only<2->{\includegraphics[width=0.45\textwidth, height=0.37\textwidth]{figures/asiatische_hornisse4.jpg}}
        \only<3->{\includegraphics[width=0.45\textwidth]{figures/asiatische_hornisse1.jpg}}
    </begin{overlayarea}
</end{columns}	
\end{frame}

